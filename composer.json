{
    "name": "apigee/apigee-php-mock-client",
    "type": "library",
    "description": "To be used for queueing mock API responses fro the `apigee/apigee-client-php` library.",
    "keywords": [
        "apigee",
        "edge",
        "api",
        "sdk",
        "client",
        "web service"
    ],
    "homepage": "http://github.com/apigee/apigee-php-mock-client",
    "license": "Apache-2.0",
    "require": {
        "php-http/client-common": "^1.9",
        "php-http/httplug": "^1.1",
        "php-http/message": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "limedeck/phpunit-detailed-printer": "^3.2",
        "php-http/discovery": "^1.0",
        "phpunit/phpunit": "^6.4.0"
    },
    "autoload": {
        "psr-4": {
            "Apigee\\MockClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apigee\\MockClient\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "update-with-dependencies": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
