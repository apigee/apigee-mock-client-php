{
    "name": "apigee/apigee-mock-client-php",
    "type": "library",
    "description": "To be used for queueing mock API responses for the `apigee/apigee-client-php` library.",
    "keywords": [
        "apigee",
        "edge",
        "api",
        "sdk",
        "client",
        "web service"
    ],
    "homepage": "http://github.com/apigee/apigee-mock-client-php",
    "license": "Apache-2.0",
    "require": {
        "guzzlehttp/psr7": "~1.0",
        "jeremeamia/superclosure": "^2.4",
        "php-http/client-common": "^2.0",
        "php-http/httplug": "^2.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "apigee/apigee-client-php": "^2.0.6",
        "limedeck/phpunit-detailed-printer": "^3.2",
        "php-http/discovery": "^1.6.0",
        "php-http/guzzle6-adapter": "^2.0",
        "phpunit/phpunit": "^6.4.0",
        "twig/twig": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Apigee\\MockClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apigee\\MockClient\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "update-with-dependencies": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
